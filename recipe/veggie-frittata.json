  "instructions": [
    {
      "step": 1,
      "description": "Preheat oven to 375°F (190°C)."
    },
    {
      "step": 2,
      "description": "In a large bowl, whisk together eggs, milk, salt, and pepper until well combined. Set aside."
    },
    {
      "step": 3,
      "description": "Heat olive oil in a 10-inch oven-safe skillet over medium heat."
    },
    {
      "step": 4,
      "description": "Add onion and cook until translucent, about 3 minutes."
    },
    {
      "step": 5,
      "description": "Add bell pepper and zucchini, and cook for another 3-4 minutes until vegetables begin to soften."
    },
    {
      "step": 6,
      "description": "Add garlic and cook for 30 seconds until fragrant."
    },
    {
      "step": 7,
      "description": "Stir in spinach and cook until just wilted, about 1 minute."
    },
    {
      "step": 8,
      "description": "Arrange the cherry tomatoes over the vegetables."
    },
    {
      "step": 9,
      "description": "Pour the egg mixture evenly over the vegetables."
    },
    {
      "step": 10,
      "description": "Sprinkle with feta cheese (if using), and dried oregano."
    },
    {
      "step": 11,
      "description": "Cook on the stovetop for 3-4 minutes until the edges begin to set."
    },
    {
      "step": 12,
      "description": "Transfer the skillet to the preheated oven and bake for 15-18 minutes, until the frittata is puffed and the center is set."
    },
    {
      "step": 13,
      "description": "Remove from oven and let cool for 5 minutes."
    },
    {
      "step": 14,
      "description": "Sprinkle with fresh basil and parsley before serving."
    },
    {
      "step": 15,
      "description": "Slice into wedges and serve warm or at room temperature."
    }
  ],
